{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "rules": {
    "array-bracket-spacing": ["error", "always"],
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "camelcase": 0,
    "comma-dangle": ["error", "always-multiline"],
    "indent": ["error", 4],
    "max-len": 0,
    "new-parens": 0,
    "no-await-in-loop": 0,
    "no-continue": 0,
    "no-param-reassign": 0,
    "no-prototype-builtins": 0,
    "no-restricted-syntax": 0,
    "object-curly-newline": ["error", { "multiline": true }],
    "operator-linebreak": ["error", "after", {
      "overrides": {
        "?": "before",
        ":": "before"
      }
    }],
    "radix": 0
  }
}
